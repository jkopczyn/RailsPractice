<%   url = session_url
button_text = "Log in" %>

<form action=<%= url %> method="post">
  <input type="hidden" 
    name="authenticity_token" value="<%= form_authenticity_token %>">
  <label>Username
    <input type="text" name="user[user_name]"> 
  </label>
  <br>
  <label>Password
   <input type="password" name="user[password]">
  </label>
  <br>
<input type="submit">
</form>
